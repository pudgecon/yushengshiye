    </div>
  <% end %>
  <% link_to "/introductions/quarry.html", class: "col-xs-4 col-md-4" do %>
    <div class="relative">
      <img src="/images/products/img33.jpg" />
      <h5><%= t("index.quarry") %><h5>
    </div>
  <% end %>
</div>
